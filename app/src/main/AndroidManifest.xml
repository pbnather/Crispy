<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dk.au.itsmap.group4.crispy">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/new_crispy_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/new_crispy_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Crispy.NoActionBar">
        <activity
            android:name=".ui.MainNavigationActivity"
            android:windowSoftInputMode="stateHidden|adjustPan">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>

        <service android:name=".service.notifications.NotificationIntentService" android:exported="false" />
    </application>

</manifest>